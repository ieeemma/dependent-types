cabal-version:      2.4
name:               dependent-types
version:            0.1.0.0


executable dependent-types
    main-is:            Main.hs
    build-depends:      base ^>=4.16.4.0
    hs-source-dirs:     src
    default-language:   GHC2021
    default-extensions: LambdaCase,
                        OverloadedStrings,
                        BlockArguments
    ghc-options:        -Wall
                        -Wcompat
                        -Wredundant-constraints
